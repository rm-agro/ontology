  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/2004/02/skos/core#Concept">
    <skos:definition xml:lang="en">An idea or notion; a unit of thought.</skos:definition>
    <rdfs:label xml:lang="en">Concept</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/2004/02/skos/core"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://rmagro.org/ontology/SeedAndPropagationMaterial">
    <skos:definition xml:lang="en">Propagating part of a plant such as seed, tubers, spore, etc.</skos:definition>
    <rdfs:label xml:lang="en">SeedAndPropagationMaterial</rdfs:label>
    <rdfs:subClassOf rdf:nodeID="A7"/>
