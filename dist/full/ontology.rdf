  <rdf:Description rdf:about="https://rmagro.org/ontology/">
    <rdfs:label xml:lang="nl">Referentiemodel Agro (rmAgro)</rdfs:label>
    <rdfs:label xml:lang="en">Referencemodel Agro (rmAgro)</rdfs:label>
    <rdfs:comment xml:lang="en">The Referencemodel Agro contains concepts and relations that specifies the agricultural domain, using existing standards and knowledge</rdfs:comment>
    <rdfs:comment xml:lang="nl">Het Referentiemodel Agro bevat concepten en relaties die het landbouw domain specificieren, gebruik makend van bestaande standaarden en kennis</rdfs:comment>
    <j.0:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-07-23</j.0:modified>
    <j.0:creator>Daoud Urdu &amp; Joep Tummers of Wageningen Economic Research</j.0:creator>
    <j.0:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-07-23</j.0:created>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Ontology"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://rmagro.org/ontology/nl.cl251">
    <rdfs:label xml:lang="en">nl.cl251</rdfs:label>
